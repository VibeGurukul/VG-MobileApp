{
  "expo": {
    "name": "VibeGurukulApp",
    "slug": "vg-mobileapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "This Example app uses the camera for video calls",
          "microphonePermission": "This Example app uses the microphone for audio and video calls",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Inter-Black.ttf",
            "./assets/fonts/Inter-Bold.ttf",
            "./assets/fonts/Inter-ExtraBold.ttf",
            "./assets/fonts/Inter-ExtraLight.ttf",
            "./assets/fonts/Inter-Light.ttf",
            "./assets/fonts/Inter-Medium.ttf",
            "./assets/fonts/Inter-Regular.ttf",
            "./assets/fonts/Inter-SemiBold.ttf",
            "./assets/fonts/Inter-Thin.ttf"
          ]
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kushagrachauhan.vgmobileapp",
      "infoPlist": {
        "UIBackgroundModes": ["audio", "location", "fetch"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kushagrachauhan.vgmobileapp",
      "permissions": [
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.CHANGE_NETWORK_STATE",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_CONNECT"
      ]
    },
    "web": {
      "favicon": "./src/assets/logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "bbc7101e-defd-4c75-8a7d-89bd03b41f0b"
      }
    },
    "owner": "kushagrachauhan"
  }
}
